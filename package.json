{
  "name": "@mismith/resume",
  "description": "Printable, JSON-driven personal CV for Murray Rowan",
  "version": "3.0.0",
  "scripts": {
    "dev": "parcel src/index.html",
    "build": "rm -rf dist/ && parcel build src/index.html --public-url ./"
  },
  "dependencies": {
    "vue": "^2.6.11"
  },
  "devDependencies": {
    "parcel": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.3.1",
    "postcss": "^8.4.31",
    "postcss-cssnext": "^3.1.0"
  },
  "postcss": {
    "plugins": {
      "postcss-cssnext": true
    }
  },
  "staticFiles": {
    "staticPath": "data",
    "watcherGlob": "**"
  }
}
